{
  "name": "analyst.js",
  "version": "0.0.1",
  "description": "Lightweight client library for making requests to Analyst Server",
  "main": "index.js",
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "scripts": {
    "develop": "watchify -v -s Analyst index.js -o test/analyst.js",
    "docs": "dox -r < index.js | doxme --readme > README.md",
    "lint": "standard index.js",
    "release": "browserify -s Analyst index.js > analyst.js",
    "test": "serve test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/conveyal/analyst.js"
  },
  "keywords": [
    "accessibility",
    "analyst",
    "leaflet"
  ],
  "author": "Trevor Gerhardt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/conveyal/analyst.js/issues"
  },
  "homepage": "https://github.com/conveyal/analyst.js",
  "devDependencies": {
    "babelify": "^6.1.0",
    "prova": "^2.1.2"
  }
}
