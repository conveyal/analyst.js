{
  "name": "analyst.js",
  "version": "0.0.4",
  "description": "Lightweight client library for making requests to Analyst Server",
  "main": "index.js",
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "scripts": {
    "build": "./node_modules/.bin/browserify -s Analyst index.js > analyst.js",
    "develop": "./node_modules/.bin/watchify -v -s Analyst index.js -o analyst.js",
    "docs": "./node_modules/.bin/dox -r < index.js | ./node_modules/.bin/doxme --readme > README.md",
    "lint": "./node_modules/.bin/standard index.js",
    "test": "./node_modules/.bin/serve"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/conveyal/analyst.js"
  },
  "keywords": [
    "accessibility",
    "analyst",
    "leaflet"
  ],
  "author": "Trevor Gerhardt",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/conveyal/analyst.js/issues"
  },
  "homepage": "https://github.com/conveyal/analyst.js",
  "devDependencies": {
    "babelify": "^6.1.1",
    "browserify": "^10.2.0",
    "dox": "^0.7.1",
    "doxme": "^1.8.2",
    "leaflet": "^0.7.3",
    "serve": "^1.4.0",
    "standard": "^4.0.1",
    "watchify": "^0.3.2"
  },
  "dependencies": {
    "debug": "^2.2.0"
  }
}
